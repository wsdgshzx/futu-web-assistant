<body>

<ul id="codeTab" class="nav nav-tabs">
  {% for item in logs %}
    {% if loop.first %}
    <li class="active">
    {% else %}
    <li>
    {% endif %}
    <a href="#{{ item.name }}" data-toggle="tab">{{ item.name }}</a> </li>
  {% endfor %}
</ul>

<div id="codeTabContent" class="tab-content">
  {% for item in logs %}
    {% if loop.first %}
    <div class="tab-pane fade in active" id="{{ item.name }}">
    {% else %}
    <div class="tab-pane fade" id="{{ item.name }}">
      {% endif %}
    <p>
    <pre id="regdemon">
      {{ item.content }}
    </pre></p>
    </div>
  {% endfor %}
</div>



 <script>
     var tabs = document.getElementsByClassName('tab-head')[0].getElementsByTagName('h2'),
         contents = document.getElementsByClassName('tab-content')[0].getElementsByTagName('div');

     (function changeTab(tab) {
         for(var i = 0, len = tabs.length; i < len; i++) {
             tabs[i].onmouseover = showTab;
         }
     })();

     function showTab() {
         for(var i = 0, len = tabs.length; i < len; i++) {
             if(tabs[i] === this) {
                 tabs[i].className = 'selected';
                 contents[i].className = 'show';
             } else {
                 tabs[i].className = '';
                 contents[i].className = '';
             }
         }
     }
</script>


